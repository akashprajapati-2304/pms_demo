{
  "name": "shiv-admin-ui-backend",
  "version": "1.0.0",
  "description": "Express.js backend API with MongoDB",
  "main": "index.js",
  "scripts": {
    "start": "nodemon backend/server.js",
    "dev": "nodemon index.js",
    "build": "npm install && cd frontend && npm install && npm run build",
    "build:dev": "webpack --mode development --progress",
    "build:standalone": "webpack --config webpack.standalone.config.js --progress",
    "build:standalone:obf": "webpack --config webpack.standalone.obfuscated.config.js --progress",
    "build:standalone:obf-lite": "webpack --config webpack.standalone.obf-lite.config.js --progress",
    "build:watch": "webpack --mode development --watch --progress",
    "serve": "node dist/server.bundle.js",
    "serve:standalone": "node dist/server.standalone.js",
    "serve:standalone:obf": "node dist/server.standalone.obf.js",
    "serve:standalone:obf-lite": "node dist/server.standalone.obf-lite.js",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "keywords": [
    "express",
    "mongodb",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb": "^6.8.0",
    "mongoose": "^8.18.3",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.0",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.5.1"
  }
}
